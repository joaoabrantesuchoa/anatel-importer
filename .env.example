# Configurações do Spectrum Board
SPECTRUM_BOARD_URL=http://localhost:3000/api/upload

# Configurações de log
LOG_LEVEL=info

# Configurações de agendamento (cron)
# Padrão: '0 * * * *' (a cada hora)
CRON_SCHEDULE=0 * * * *

# Diretório de trabalho para dados temporários
DATA_DIR=./anatel_data

# Timeout para requisições HTTP (em ms)
HTTP_TIMEOUT=30000
